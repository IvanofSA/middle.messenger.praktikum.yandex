{
  "compilerOptions": {
    "outDir": "./dist",
    "target": "es5",
    "declaration": false,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "noEmitOnError": false
  },
  "lib": ["es2019.object", "es2019", "dom"],
  "exclude": ["dist", "node_modules", "src/**/*.spec.ts"]
}
